{
  "extends": "../tslint.json",
  "rules": {
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
